nodes-test
    div.some-class AS $d
        "Hello"

    ^$d
        " "
        span
            "World"

    ^(d)
        @aaa "bbb"

    ^(d)
        EACH $val ([1, 2, 3, 4])
            TEST (val % 2 === 0)
                ATTR ('ccc' + val) ('dd' + 'd')

    EACH $val ([1, 2, 3, 4])
        ^$d
            TEST (val % 2 === 1)
                ATTR ('eee' + val) "fff"
