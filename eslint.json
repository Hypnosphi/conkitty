{
    "extends": "eslint:recommended",
    "rules": {
        "quotes": [2, "single"],
        "no-shadow-restricted-names": 0,
        "no-underscore-dangle": 0,
        "no-use-before-define": [2, "nofunc"],
        "no-new": 0,
        "new-cap": 0,
        "no-multi-spaces": 0,
        "comma-spacing": 0
    },
    "env": {
        "node": true
    }
}
